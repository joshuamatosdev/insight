{
  "name": "insight-dashboard",
  "private": true,
  "version": "1.0.0",
  "description": "Insight Contract Intelligence Platform - Frontend Dashboard",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:fresh": "node -e \"require('fs').rmSync('node_modules/.vite',{recursive:true,force:true})\" && vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:naming": "ls-lint",
    "typecheck": "node ./node_modules/typescript/bin/tsc -p tsconfig.app.json --noEmit",
    "typecheck:watch": "node ./node_modules/typescript/bin/tsc -p tsconfig.app.json --noEmit --watch",
    "check-types": "node ./node_modules/typescript/bin/tsc -p tsconfig.app.json --noEmit",
    "check-types:watch": "node ./node_modules/typescript/bin/tsc -p tsconfig.app.json --noEmit --watch",
    "validate": "npm run typecheck && npm run lint && npm run lint:naming",
    "validate:fix": "npm run typecheck && npm run lint:fix && npm run lint:naming",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "preview": "vite preview",
    "generate:types": "node scripts/generate-api-types.js",
    "generate:types:save": "node scripts/generate-api-types.js --save-spec",
    "generate:types:offline": "node scripts/generate-api-types.js --from-file"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "clsx": "^2.1.1",
    "framer-motion": "^12.29.2",
    "motion": "^12.29.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^7.6.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@axe-core/react": "^4.11.0",
    "@eslint/js": "^9.39.1",
    "@ls-lint/ls-lint": "^2.3.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.5",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.5.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.4.4",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^3.2.4"
  }
}
