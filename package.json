{
  "name": "samgov",
  "version": "1.0.0",
  "description": "SAMGov Contract Intelligence Platform",
  "type": "module",
  "scripts": {
    "git:agent": "node --import=tsx scripts/git-agent-runner.ts",
    "git:merge": "node --import=tsx scripts/merge-manager.ts",
    "git:merge:list": "node --import=tsx scripts/merge-manager.ts list",
    "wave:run": "node --import=tsx scripts/wave-executor.ts run",
    "wave:status": "node --import=tsx scripts/wave-executor.ts status",
    "wave:list": "node --import=tsx scripts/wave-executor.ts list"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
